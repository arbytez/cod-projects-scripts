{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon -e js,graphql --ignore src/tests/* -x node --inspect src/index.js",
    "pm2": "cross-env NODE_ENV=production pm2 start ./src/index.js -n Cod4GraphQLServer",
    "prisma:deploy": "rimraf ./src/server/generated && prisma deploy --env-file .env.prisma",
    "prisma:token": "prisma token --env-file .env.prisma",
    "prisma:generate": "rimraf ./src/server/generated && prisma generate --env-file .env.prisma",
    "prisma:admin": "prisma admin --env-file .env.prisma",
    "prisma:playground": "prisma playground --env-file .env.prisma",
    "test": "cross-env NODE_ENV=test jest",
    "majestic": "cross-env NODE_ENV=test majestic --port=4001 --noOpen"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^6.0.3",
    "easygraphql-tester": "^5.1.6",
    "jest": "^24.9.0",
    "majestic": "^1.6.0",
    "nodemon": "^1.19.3",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "apollo-server-express": "^2.9.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.4.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.0.0",
    "graphql": "^14.5.8",
    "graphql-cli": "^3.0.14",
    "graphql-import": "^0.7.1",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^3.21.1",
    "hpp": "^0.2.2",
    "iconv-lite": "^0.5.0",
    "joi-objectid": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^1.34.8",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.8",
    "signale": "^1.4.0",
    "sqlite3": "^4.1.0",
    "ssh2": "^0.8.5",
    "xss-clean": "^0.1.1"
  },
