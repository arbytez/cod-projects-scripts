{
  "name": "cod4dashboard-frontend",
  "version": "1.0.0",
  "description": "Cod4dashboard frontend",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "build": "cross-env NODE_ENV=production npm run clean && next build",
    "export": "cross-env NODE_ENV=production npm run build && next export",
    "start": "cross-env NODE_ENV=production next start",
    "clean": "rimraf ./out && rimraf ./.next",
    "generate": "rimraf ./src/generated/js && npm-run-all graphqlcodegen tsc",
    "graphqlcodegen": "graphql-codegen",
    "tsc": "tsc -p ./src/config/tsconfig.codegen.json"
  },
  "license": "MIT",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-link-batch-http": "^1.2.13",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.19",
    "cross-env": "^6.0.3",
    "date-fns": "^2.6.0",
    "dotenv-webpack": "^1.7.0",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "js-cookie": "^2.2.1",
    "next": "^9.1.1",
    "next-with-apollo": "^4.3.0",
    "nookies": "^2.0.8",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "react-use": "^12.7.2",
    "subscriptions-transport-ws": "^0.9.16"
  },
  "devDependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@graphql-codegen/cli": "^1.8.2",
    "@graphql-codegen/typescript": "^1.8.2",
    "@graphql-codegen/typescript-graphql-request": "^1.8.2",
    "@graphql-codegen/typescript-operations": "^1.8.2",
    "@graphql-codegen/typescript-react-apollo": "^1.8.2",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/js-cookie": "^2.2.4",
    "autoprefixer": "9.6.5",
    "cssnano": "4.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "tailwindcss": "1.1.3",
    "typescript": "^3.6.4"
  }
}
